pipeline { 
    agent any    
    stages { 
        stage('Ejecucion de pruebas') {  
            steps { 
                sh "newman run prueba.json -r htmlextra"
            } 
        }
		stage('Publicar reporte') {
			steps{
				publishHTML([allowMissing: false, alwaysLinkToLastBuild: false, keepAll: false, reportDir: '', reportFiles: '*.html', reportName: 'Reporte de Ejecucion', reportTitles: ''])
			}
		}	
    } 
}
